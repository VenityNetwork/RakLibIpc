{
	"name": "venitynetwork/raklib-ipc",
	"description": "Channel-based protocols for inter-thread/inter-process communication with RakLib",
	"type": "library",
	"license": "GPL-3.0",
	"require": {
		"php": "^8.0",
		"php-64bit": "*",
		"venitynetwork/binaryutils": "^0.2.0",
		"venitynetwork/raklib": "^0.13.1 || ^0.14.0"
	},
	"require-dev": {
		"phpstan/phpstan": "1.10.1",
		"phpstan/phpstan-strict-rules": "^1.0.0"
	},
	"autoload": {
		"psr-4": {
			"raklib\\server\\ipc\\": "src/"
		}
	},
	"repositories": [
		{
			"type": "vcs",
			"url":  "git@github.com:VenityNetwork/BinaryUtils.git"
		},
		{
			"type": "vcs",
			"url":  "git@github.com:VenityNetwork/RakLib.git"
		}
	]
}
